{{define "messages"}}
pub trait Encode {
    fn encode (self, b: &mut Cursor<Vec<u8>>) -> Result<&mut Cursor<Vec<u8>>, io::Error>;
}

pub trait Decode {
    fn decode (b: &mut Cursor<&mut Vec<u8>>) -> Result<Option<Self>, DecodingError> where Self: Sized;
}
{{range $i, $e := (MakeIterable .messages.Len) -}}
    {{ $message := $.messages.Get $i }}
